.wrap {
  display: inline-flex
  max-width: 100%
  white-space: nowrap
  align-items: baseline;
}
.Hash {
  display: inline-flex
  max-width: 100%
  overflow: hidden
  align-items: baseline;
  font-family: "Lucida Console", Courier, Monaco, monospace, serif;
}

.before {
  overflow: hidden
  white-space: nowrap
  text-overflow: ellipsis;
  text-align: left
}

.after {
  white-space: nowrap
}

.message {
  max-width: 100%;
  overflow: hidden
  text-overflow: ellipsis;
}
.copyIcon,
.linkIcon{
  vertical-align: middle
  align-self: center
}
