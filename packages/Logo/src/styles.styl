.wrap {
  display: inline-block
  text-decoration: none
  outline: none
  border: none
  box-shadow none
  vertical-align: middle
  &:focus,
  &:active,
  &:hover {
    text-decoration: none
    outline: none
    border: none
    box-shadow none
  }
}
.svg {
  max-width: 100%
  max-height 100%
  height: auto
  width: 100%
}
